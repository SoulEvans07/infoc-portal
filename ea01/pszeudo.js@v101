"use strict";function debuggerlike(mit){this.elem=function(i){return $(this.melyik+i)};this.jelol=function(){for(var i=1;this.elem(i)!=null;i++)
this.elem(i).classList.remove("aktiv");if(this.aktualis!=0)
this.elem(this.aktualis).classList.add("aktiv")}
this.kovetkezo=function(){this.aktualis=this.csinal();if(this.elem(this.aktualis)==null){this.aktualis=0}
this.jelol()}
this.melyik=mit;this.aktualis=0;this.jelol();var gomb=this.elem("kovetkezo"),dis=this;addEvent(gomb,'click',function(event){dis.kovetkezo()})}
function szekvobj(mit){debuggerlike.call(this,mit);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="\n\n\n";this.elem("a").innerHTML="";this.a=Math.round(Math.random()*8+2);return 1;case 1:this.elem("screenshot").innerHTML="Kérem a számot!\n<blink>_<\/blink>\n\n";return 2;case 2:this.elem("screenshot").innerHTML="Kérem a számot!\n"+this.a+"\n\n";this.elem("a").innerHTML="<strong>"+this.a+"<\/strong>";return 3;case 3:this.elem("a").innerHTML="<strong>"+this.a*this.a+"<\/strong>";return 4;case 4:this.elem("screenshot").innerHTML="Kérem a számot!\n"+this.a+"\n"+this.a*this.a+"\n";this.elem("a").innerHTML=this.a*this.a;return 0}}}
function feltbobj(mit){debuggerlike.call(this,mit);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="<blink>_<\/blink>\n\n\n";this.elem("a").innerHTML="";this.a=Math.round(Math.random()*10);return 1;case 1:this.elem("screenshot").innerHTML=""+this.a+"\n\n\n";this.elem("a").innerHTML=this.a;return 2;case 2:if(this.a%2==0)
return 3;else return 4;case 3:this.elem("screenshot").innerHTML=""+this.a+"\npáros\n\n";this.aktualis++;return 0;case 4:this.elem("screenshot").innerHTML=""+this.a+"\npáratlan\n\n";return 0}}}
function ciklaobj(mit){debuggerlike.call(this,mit);this.csinal=function(){switch(this.aktualis){case 0:this.elem("screenshot").innerHTML="\n\n";this.elem("i").innerHTML="";this.elem("n").innerHTML="";this.szamki="";return 1;case 1:this.elem("screenshot").innerHTML="Meddig írjam ki? <blink>_<\/blink>\n\n";return 2;case 2:this.n=5;this.elem("n").innerHTML=this.n;this.elem("screenshot").innerHTML="Meddig írjam ki? "+this.n+"\n\n";return 3;case 3:this.i=1;this.elem("i").innerHTML=this.i;return 4;case 4:if(this.i<=this.n)
return 5;else return 7;case 5:this.szamki+=this.i+" ";this.elem("screenshot").innerHTML="Meddig írjam ki? "+this.n+"\n"+this.szamki+"\n";return 6;case 6:this.i++;this.elem("i").innerHTML=this.i;return 4;case 7:this.szamki+=".";this.elem("screenshot").innerHTML="Meddig írjam ki? "+this.n+"\n"+this.szamki+"\n";return 0}}}
infoc.onloads.push(function(){infoc.ea02={szekvpeld:new szekvobj('szekv'),feltbpeld:new feltbobj('feltb'),ciklapeld:new ciklaobj('cikla'),}})